<?xml version="1.0"?>
<root>
  <vkopenurldef>
    <name>KeyCode::VK_OPEN_URL_Emacs</name>
    <url type="file">/Applications/Emacs.app</url>
  </vkopenurldef>
  <vkopenurldef>
    <name>KeyCode::VK_OPEN_URL_iTerm</name>
    <url type="file">/Applications/iTerm.app</url>
  </vkopenurldef>
  <vkopenurldef>
    <name>KeyCode::VK_OPEN_URL_Chrome</name>
    <url type="file">/Applications/Google Chrome.app</url>
  </vkopenurldef>
  <vkopenurldef>
    <name>KeyCode::VK_OPEN_URL_OmniFocus</name>
    <url type="file">/Applications/OmniFocus.app</url>
  </vkopenurldef>
  <vkopenurldef>
    <name>KeyCode::VK_OPEN_URL_Skype</name>
    <url type="file">/Applications/Skype.app</url>
  </vkopenurldef>
  <vkopenurldef>
    <name>KeyCode::VK_OPEN_URL_Slack</name>
    <url type="file">/Applications/Slack.app</url>
  </vkopenurldef>
  <item>
    <name>Switch to Emacs</name>
    <identifier>switch.emacs</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::D, ModifierFlag::COMMAND_L | ModifierFlag::CONTROL_L,
      KeyCode::VK_OPEN_URL_Emacs
    </autogen>
  </item>
  <item>
    <name>Switch to iTerm</name>
    <identifier>switch.iterm</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::K, ModifierFlag::COMMAND_L | ModifierFlag::CONTROL_L,
      KeyCode::VK_OPEN_URL_iTerm
    </autogen>
  </item>
  <item>
    <name>Switch to Chrome</name>
    <identifier>switch.chrome</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::I, ModifierFlag::COMMAND_L | ModifierFlag::CONTROL_L,
      KeyCode::VK_OPEN_URL_Chrome
    </autogen>
  </item>
  <item>
    <name>Switch to OmniFocus</name>
    <identifier>switch.of</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::S, ModifierFlag::COMMAND_L | ModifierFlag::CONTROL_L,
      KeyCode::VK_OPEN_URL_OmniFocus
    </autogen>
  </item>
  <item>
    <name>Switch to Skype</name>
    <identifier>switch.skype</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::SEMICOLON, ModifierFlag::COMMAND_L | ModifierFlag::CONTROL_L,
      KeyCode::VK_OPEN_URL_Skype
    </autogen>
  </item>
  <item>
    <name>Switch to Slack</name>
    <identifier>switch.slack</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::P, ModifierFlag::COMMAND_L | ModifierFlag::CONTROL_L,
      KeyCode::VK_OPEN_URL_Slack
    </autogen>
  </item>
  <item>
            <name>Change input source to Russian while you are holding down right Cmd key.</name>
            <identifier>remap.change_input_source_temporary_russian_cmd_r</identifier>
            <autogen>
                __KeyToKey__
                KeyCode::COMMAND_R,
                KeyCode::VK_NONE,
                Option::KEYTOKEY_BEFORE_KEYDOWN, KeyCode::VK_CHANGE_INPUTSOURCE_RUSSIAN, KeyCode::VK_WAIT_10MS,
                Option::KEYTOKEY_AFTER_KEYUP, KeyCode::VK_CHANGE_INPUTSOURCE_DVORAK, KeyCode::VK_WAIT_10MS,
            </autogen>
        </item>
        <item>
          <name>Change Command+Tab to Command+Control+Option+Shift+Tab</name>
          <identifier>remap.change_command_tab_to_ccos_tab</identifier>
          <autogen>
            __KeyToKey__
            KeyCode::TAB, ModifierFlag::COMMAND_L,
            KeyCode::TAB, ModifierFlag::COMMAND_L | ModifierFlag::OPTION_L | ModifierFlag::CONTROL_L
          </autogen>
          <autogen>
            __KeyToKey__
            KeyCode::TAB, ModifierFlag::COMMAND_L | ModifierFlag::SHIFT_L,
            KeyCode::TAB, ModifierFlag::COMMAND_L | ModifierFlag::SHIFT_L | ModifierFlag::OPTION_L | ModifierFlag::CONTROL_L
          </autogen>
        </item>
</root>
