{
  "title": "Launch apps",
  "rules": [
    {
      "description": "New iTerm2 window",
      "manipulators": [
        {
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["left_command", "left_control", "left_shift"]
            }
          },
          "to": [{"shell_command": "$HOME/bin/iterm-new-window"}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Activate iTerm2",
      "manipulators": [
        {
          "from": {
            "key_code": "k",
            "modifiers": {"mandatory": ["left_command", "left_control"]}
          },
          "to": [{"shell_command": "open -a iTerm"}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Activate Chrome",
      "manipulators": [
        {
          "from": {
            "key_code": "i",
            "modifiers": {"mandatory": ["left_command", "left_control"]}
          },
          "to": [{"shell_command": "open -a 'Google Chrome'"}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Activate Emacs",
      "manipulators": [
        {
          "from": {
            "key_code": "d",
            "modifiers": {"mandatory": ["left_command", "left_control"]}
          },
          "to": [{"shell_command": "open -a Emacs"}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Activate Finder",
      "manipulators": [
        {
          "from": {
            "key_code": "y",
            "modifiers": {"mandatory": ["left_command", "left_control"]}
          },
          "to": [{"shell_command": "open -a Finder"}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Activate Calendar",
      "manipulators": [
        {
          "from": {
            "key_code": "p",
            "modifiers": {"mandatory": ["left_command", "left_control"]}
          },
          "to": [{"shell_command": "open -a Calendar"}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Activate OmniFocus",
      "manipulators": [
        {
          "from": {
            "key_code": "j",
            "modifiers": {"mandatory": ["left_command", "left_control"]}
          },
          "to": [{"shell_command": "open -a OmniFocus"}],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Activate MailMate",
      "manipulators": [
        {
          "from": {
            "key_code": "a",
            "modifiers": {"mandatory": ["left_command", "left_control"]}
          },
          "to": [{"shell_command": "open -a MailMate"}],
          "type": "basic"
        }
      ]
    }
  ]
}
